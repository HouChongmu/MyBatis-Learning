<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yolyn.chapter031.mapper.MaleHealthFormMapper">
	<select id="getMaleHealthForm" parameterType="long"
		resultType="com.yolyn.chapter031.pojo.MaleHealthForm">
		select id, emp_id as empId,heart, liver, spleen, lung, kidney, prostate, note from
		t_male_health_form_ssm where emp_id = #{id}
	</select>
</mapper>